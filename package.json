{
  "name": "ts-express-mongodb",
  "version": "1.0.0",
  "description": "A boilerplate for typescript, express and mongodb project",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --ignore-path .prettierignore --check .",
    "prettier:fix": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "lint-prettier:check": "npm run lint:check && npm run prettier:check",
    "lint-prettier:fix": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*ts": "npm run lint-prettier:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MdEstiakAhmed/ts-express-mongodb-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "express",
    "mongodb",
    "boilerplate"
  ],
  "author": "Md. Estiak Ahmed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MdEstiakAhmed/ts-express-mongodb-boilerplate/issues"
  },
  "homepage": "https://github.com/MdEstiakAhmed/ts-express-mongodb-boilerplate#readme",
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.1"
  }
}
